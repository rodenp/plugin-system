{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@lib/*": ["src/lib/*"],
      "@core/*": ["src/core/*"],
      "@types/*": ["src/types/*"],
      "@store/*": ["src/store/*"],
      "@plugin-shared/*": ["src/plugins/shared/*"],
      "@plugin-course/*": ["src/plugins/course-builder/*"],
      "@plugin-community/*": ["src/plugins/community/*"],
      "@plugin-community-dashboard/*": ["src/plugins/community-dashboard/*"],
      "@plugin-classroom/*": ["src/plugins/classroom/*"],
      "@plugin-calendar/*": ["src/plugins/calendar/*"],
      "@plugin-about/*": ["src/plugins/about/*"],
      "@plugin-merchandise/*": ["src/plugins/merchandise/*"],      
      "@plugin-members/*": ["src/plugins/members/*"],   
      "@plugin-forum/*": ["src/plugins/forum-builder/*"]
    }
  }
}